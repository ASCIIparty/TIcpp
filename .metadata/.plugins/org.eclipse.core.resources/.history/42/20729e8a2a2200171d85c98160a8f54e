#include "tadress.h"

unsing namespace std;

TAdress::TAdress()
{

}

TAdress(string, string, string, string);


void TAdress::print()
{
	printf("%s %s%n", getStreet(), getNumber());
	printf("%s %s%n", getZipcode(), getTown());
}

string TAdress::getStreet()
{
	return street;
}

string TAdress::getNumber()
{
	return number;
}

string TAdress::getZipcode()
{
	return zipcode;
}

string TAdress::getTown()
{
	return town;
}

