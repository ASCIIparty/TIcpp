#ifndef TADDRESS_H_
#define TADDRESS_H_

using namespace std;

class TAdress {

private:
	string street;
	string number;
	string zipcode;
	string town;
public:
	TAdress();
	TAdress(string, string, string, string);
	void print();
	string getStreet();
	string getNumber();
	string getZipcode();
	string getTown();
	void setStreet(string);
	void setNumber(string);
	void setZipcode(string);
	void setTown(string);
	~TAdress();

};

#endif /* TADDRESS_H_ */
