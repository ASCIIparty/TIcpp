#include "tadress.h"

unsing namespace std;

TAdress::TAdress() {

}

void TAdress::print()
{
	printf("%s %s%n", getStreet(), getNumber());
	printf("%s %s%n", getZipcode(), getTown())
}

string TAdress::getStreet()
{
	return street;
}

string getNumber()
{
	return number;
}

string getZipcode()
{
	return zipcode;
}

string getTown()
{
	return town;
}

