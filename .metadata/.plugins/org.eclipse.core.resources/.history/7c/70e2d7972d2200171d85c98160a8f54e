#include "tperson.h"

TPerson::TPerson() {
	// TODO Auto-generated constructor stub

}

TPerson::TPerson(string n , TAddress a, TDate b)
{
	name = n;
	address = a;
	birthday = b;
}

void TPerson::print()
{
	printf("%s%n", getName());
	address.print();
	printf("%s", getBirthday());
}
string getName();
TAddress getAddress();
TDate getBirthday();
void setName(string);
void setAddress(TAddress);
void setBirthday(TDate);

TPerson::~TPerson() {
	// TODO Auto-generated destructor stub
}

