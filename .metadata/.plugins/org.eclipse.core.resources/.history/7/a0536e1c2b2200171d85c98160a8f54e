#ifndef TADRESS_H_
#define TADRESS_H_

using namespace std;

class TAdress {

private:
	string street;
	string number;
	string zipcode;
	string town;
public:
	TAdress();
	TAdress(string, string, string, string);
	void print();
	string getStreet();
	string getNumber();
	string getZipcode();
	string getTown();
	string setStreet();
	string setNumber();
	string setZipcode();
	string setTown();
	~TAdress();

};

#endif /* TADRESS_H_ */
