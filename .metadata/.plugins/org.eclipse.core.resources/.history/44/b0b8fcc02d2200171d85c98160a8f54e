#include "tperson.h"

TPerson::TPerson() {
	// TODO Auto-generated constructor stub

}

TPerson::TPerson(string n , TAddress a, TDate b)
{
	name = n;
	address = a;
	birthday = b;
}

void TPerson::print()
{
	printf("%s%n", getName());
	address.print();
	printf("%s", getBirthday());
}
string TPerson::getName()
{
	return name;
}

TAddress TPerson::getAddress()
{
	address.print();
}

TDate TPerson::getBirthday()
{
	birthday.print();
}

void TPerson::setName(string n)
{
	name = n;
}

void TPerson::setAddress(TAddress a)
{
	address = a;
}
void setBirthday(TDate);

TPerson::~TPerson() {
	// TODO Auto-generated destructor stub
}

