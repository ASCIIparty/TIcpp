#include "tlocation.h"

unsing namespace std;

TLocation::TLocation()
{
	section = "BÃ¼ro";
	rack = "Fach zum Einsortieren";
}

TLocation::TLocation(string s, string r)
{
	section = s;
	rack = r;
}

void TLocation::print()
{
	printf("Abt.: %s; Regal: %s%n", getSection(), getRack());
}

string TLocation::getSection()
{
	return section;
}

string TLocation::getRack()
{
	return rack;
}

